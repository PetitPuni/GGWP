<div class="ranking" data-ranking-subscription-target="ranking">
<% player_rankings.each_with_index do |player, index| %>
  <tr>
    <td><%= index + 1 %></td>
    <td><%= player.steam_username %></td>
    <!--<td><%= player.challenges.name %></td>-->
    <td><%= player.score %></td>
  </tr>
<% end %>
</div>
